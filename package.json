{
  "name": "nadia-web-starter-kit",
  "version": "4.0.0",
  "scripts": {
    "start": "run-p serve dev",
    "dev": "run-s clean:dist watch",
    "test": "run-p serve check",
    "check": "run-s clean:dist copy imagemin build:* clean:sourcemaps vrt",
    "build": "run-s clean:dist copy imagemin build:* clean:sourcemaps",
    "build:styles": "webpack --mode=production --config webpack.css.js",
    "build:scripts": "webpack --mode=production",
    "build:html": "run-p pug ejs",
    "clean:dist": "dotenv -- cross-var \"del-cli -f \"htdocs/**\" \"wp-content/themes/%SITE_NAME%/_assets/\" \"!htdocs/**/.*\" \"!htdocs/wp\"\"",
    "clean:sourcemaps": "del-cli -f \"htdocs/**/*.map\"",
    "watch": "run-p watch:*",
    "watch:styles": "npm run styles",
    "watch:scripts": "npm run scripts -- -w",
    "watch:pug": "onchange -i \"src/**/(*.html|*.pug)\" -- npm run pug",
    "watch:ejs": "onchange -i \"src/**/*.ejs\" -- npm run ejs",
    "watch:images": "onchange -i \"src/_assets/img/**/*.{jpg,jpeg,png,gif,svg}\" -- npm run imagemin {{changed}}",
    "watch:copy": "onchange -i \"src/**/!(*.js|*.scss|*.pug|*.ejs|*.jpg|*.jpeg|*.png|*.gif|*.svg)\" -- npm run copy",
    "serve": "browser-sync start -c bs-config.js",
    "styles": "webpack --mode=development --config webpack.css.js --watch",
    "scripts": "webpack --mode=development",
    "pug": "pug --out htdocs --basedir src/pug --pretty src/pug",
    "pug2": "node pug.js",
    "ejs": "ejs-cli --base-dir src \"/**/{_tpl_,!(_)}*.ejs\" -o htdocs",
    "imagemin": "node imagemin.js",
    "copy": "copyfiles -u 1 \"src/**/!(*.js|*.scss|*.pug|*.ejs|*.jpg|*.jpeg|*.png|*.gif|*.svg|_*.html)\" htdocs",
    "vrt": "node visual-test.mjs"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "author": "nadia Inc.",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "autoprefixer": "^10.4.1",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.27.7",
    "clean-css-cli": "^5.5.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.20.2",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "css-loader": "^6.7.1",
    "del-cli": "^4.0.1",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.1.0",
    "ejs-cli": "^2.2.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "fs-extra": "^3.0.1",
    "glob": "^7.2.0",
    "glob-import-loader": "^1.2.0",
    "html5-reset": "^2.1.3",
    "imagemin-cli": "^7.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "imagemin-webp": "^6.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "pug-cli": "github:pugjs/pug-cli#master",
    "puppeteer": "13.5.1",
    "purgecss-webpack-plugin": "^5.0.0",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "sharp": "0.30.3",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-scss": "^4.2.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-fix-style-only-entries": "^0.6.1"
  },
  "devDependencies": {
    "@lit-labs/motion": "^1.0.4",
    "@types/hammerjs": "^2.0.40",
    "@types/jquery": "^3.5.16",
    "@types/jquery-match-height": "^0.7.6",
    "@types/micromodal": "^0.3.3",
    "@types/slick-carousel": "^1.6.37",
    "details-element-polyfill": "^2.4.0",
    "dialog-polyfill": "^0.5.6",
    "hammerjs": "^2.0.8",
    "jquery": "^3.7.0",
    "jquery-match-height": "^0.7.2",
    "js-cookie": "^3.0.1",
    "lit": "^2.8.0",
    "lity": "^2.4.1",
    "micromodal": "^0.4.10",
    "particles.js": "^2.0.0",
    "slick-carousel": "^1.8.1"
  }
}
