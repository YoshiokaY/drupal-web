{
  "name": "nadia-web-starter-kit",
  "version": "4.0.0",
  "scripts": {
    "start": "run-p serve dev",
    "dev": "run-s clean:dist watch",
    "test": "run-s clean:dist copy imagemin build:* clean:sourcemaps changePath",
    "build": "run-s clean:dist copy imagemin build:* clean:sourcemaps",
    "build:styles": "webpack --mode=production --config webpack.css.js",
    "build:scripts": "webpack --mode=production",
    "build:html": "run-p pug ejs",
    "clean:dist": "dotenv -- cross-var \"del-cli -f \"htdocs/**\" \"!htdocs/**/.*\"\"",
    "clean:sourcemaps": "del-cli -f \"htdocs/**/*.map\"",
    "changePath": "./changePath.sh",
    "watch": "run-p watch:*",
    "watch:styles": "npm run styles",
    "watch:scripts": "npm run scripts -- -w",
    "watch:pug": "onchange -i \"src/**/(*.html|*.pug)\" -- npm run pug {{changed}}",
    "watch:ejs": "onchange -i \"src/**/*.ejs\" -- npm run ejs",
    "watch:images": "onchange -i \"src/_assets/img/**/*.*\" -- npm run imagemin {{changed}}",
    "watch:copy": "onchange -i \"src/**/!(*.js|*.scss|*.pug|*.ejs|*.html|*.ts|*.css)\" -e \"src/_assets/img/*.*\" -- npm run copy",
    "serve": "browser-sync start -c bs-config.js",
    "styles": "webpack --mode=development --config webpack.css.js --watch",
    "scripts": "webpack --mode=development",
    "pug": "pug src/pug -o htdocs/ -P",
    "ejs": "ejs-cli --base-dir src \"/**/{_tpl_,!(_)}*.ejs\" -o htdocs",
    "imagemin": "node imagemin.js",
    "copy": "copyfiles -e \"src/_assets/img/*.*\" -u 1 htdocs"
  },
  "browserslist": [
    "last 2 version"
  ],
  "author": "nadia Inc.",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "autoprefixer": "^10.4.1",
    "babel-loader": "^8.2.3",
    "browser-sync": "^2.27.7",
    "clean-css-cli": "^5.5.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.20.2",
    "del-cli": "^4.0.1",
    "dotenv": "^16.0.0",
    "ejs-cli": "^2.2.3",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "fs-extra": "^3.0.1",
    "html5-reset": "^2.1.3",
    "imagemin-cli": "^7.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "intersection-observer": "^0.12.2",
    "jquery": "^3.6.0",
    "jquery-match-height": "^0.7.2",
    "js-cookie": "^3.0.1",
    "lit": "^2.0.2",
    "lity": "^2.4.1",
    "micromodal": "^0.4.10",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "object-fit-images": "^3.2.4",
    "onchange": "^7.1.0",
    "particles.js": "^2.0.0",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.5.1",
    "pug-cli": "github:pugjs/pug-cli#master",
    "slick-carousel": "^1.8.1",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-scss": "^4.2.0",
    "ts-loader": "^8.2.0",
    "typescript": "^4.4.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "css-loader": "^6.7.1",
    "dotenv-cli": "^5.1.0",
    "glob": "^7.2.0",
    "glob-import-loader": "^1.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.14",
    "postcss-loader": "^6.2.1",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "webpack-fix-style-only-entries": "^0.6.1"
  }
}
